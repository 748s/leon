<form method="post"{% if form.id %} id="{{ form.id }}"{% endif %}{% if form.classes %} class="{{ form.classes|join(' ') }}"{% endif %}{% if form.action %} action="{{ form.action }}"{% endif %}{% if form.multipartFormData %} enctype="multipart/form-data"{% endif %}>
    {% include '@Leon/component/alert.html.twig' with {'alert': form.alert} %}
    {% if form.title %}<h3>{{ form.title }}</h3>{% endif %}
    {% for element in form.elements %}
        {% include element.template %}
    {% endfor %}
    <div class="form-group">
        <button type="submit" class="btn btn-primary btn-block">{{ form.submitButtonText }}</button>
    </div>
    <script>
        $('select').selectpicker();
        {% if form.useJqueryValidate %}$("#{{ form.id }}").validate();{% endif %}
    </script>
</form>
